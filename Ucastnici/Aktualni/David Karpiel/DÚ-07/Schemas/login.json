{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "response": {
      "type": "string"
    },
    "reason": {
      "type": "string",
      "const": "correct"
    },
    "agent": {
      "type": "object",
      "properties": {
        "browser": {
          "type": "string"
        },
        "version": {
          "type": "string"
        }
      },
      "required": [
        "browser",
        "version"
      ]
    }
  },
  "required": [
    "response",
    "reason",
    "agent"
  ]
}